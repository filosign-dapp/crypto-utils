{
    "name": "filosign-crypto-utils",
    "version": "0.2.10",
    "description": "Crypto utilities for FiloSign",
    "main": "exports.ts",
    "types": "exports.ts",
    "exports": {
        ".": {
            "import": "./exports.ts",
            "require": "./pkg-node/filosign_crypto_utils.js",
            "types": "./exports.ts"
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/filosign-dapp/crypto-utils.git"
    },
    "keywords": [
        "cryptography",
        "wasm",
        "filosign",
        "encryption"
    ],
    "scripts": {
        "build": "wasm-pack build --target web --out-dir pkg; wasm-pack build --target nodejs --out-dir pkg-node",
        "test": "npm run build && bun run test.ts",
        "prepublishOnly": "npm run build"
    },
    "devDependencies": {
        "@types/bun": "^1.2.21"
    }
}